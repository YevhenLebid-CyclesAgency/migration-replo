                      d="M22.3705 8.66443C22.3696 8.66644 22.3685 8.67031 22.3685 8.67716C22.3685 8.68399 22.3696 8.68785 22.3705 8.68986C22.3715 8.69217 22.3726 8.69415 22.3743 8.69586C22.3759 8.69757 22.3777 8.69912 22.3805 8.70033C22.3828 8.70141 22.3873 8.70281 22.3942 8.70281H30.1195C30.1267 8.70281 30.1311 8.70141 30.1334 8.70033C30.136 8.69912 30.138 8.69757 30.1397 8.69586C30.1413 8.69415 30.1425 8.69217 30.1434 8.68986C30.1441 8.68785 30.1452 8.68399 30.1452 8.67716C30.1452 8.67031 30.1441 8.66644 30.1434 8.66443C30.1425 8.66215 30.1413 8.66016 30.1397 8.65846C30.138 8.65673 30.136 8.65517 30.1334 8.65396C30.1311 8.65289 30.1267 8.65148 30.1195 8.65148H22.3942C22.3873 8.65148 22.3828 8.65289 22.3805 8.65396C22.3777 8.65517 22.3759 8.65673 22.3743 8.65846C22.3726 8.66016 22.3715 8.66215 22.3705 8.66443ZM21.4364 8.67716C21.4364 8.10084 21.864 7.64355 22.3942 7.64355H30.1195C30.6499 7.64355 31.0775 8.10084 31.0775 8.67716C31.0775 9.25345 30.6499 9.71074 30.1195 9.71074H22.3942C21.864 9.71074 21.4364 9.25345 21.4364 8.67716Z"
                      fill="#1E415F" />
              
              
          </svg>' %}
        {% assign planId = '' %}
      {% endif %}

      {% if variant.title == '3 Pack' or variant.title == '3 Pack / Gentle' %}
        {% assign active = 'active' %}
      {% endif %}

      {% assign extra = '' %}

      {% if variant.title contains 'Extra' %}
        {% assign extra = 'swatchCustom__item_new__extra' %}
      {% endif %}

      <div
        class="
          swatchCustom__item_new flx {% if variant.title contains " 1 Pack" %} nosale {% endif %} {{active}}
          {{ extra }}
        "
        data-variant="{{variant.id}}"
        data-title="{{variant.title}}"
        data-price="{{price | divided_by: 100.0}}"
        data-planid="{{planId}}"
        data-week="{{ week }}"
        data-compare="{{compare}}"
        data-strips="{{strips}}"
      >
        <div class="flx">
          {{ icon }}
          <div>
            <p class="for-week">{{ price_week }} / week</p>
            <p class="item_total">
              Total:
              {% if variant.title != '1 Pack'
                and variant.title != '1 Pack / Gentle'
                and variant.title != '1 Pack / Extra'
              -%}
                <span class="l-through"> {{ compare }} </span>
              {%- endif %}
              <span>{{ price | money }}</span>
            </p>
          </div>
        </div>
        <div>
          {% if variant.title contains '12 Pack' %}
            <div class="best-deal">Best deal</div>
          {% elsif variant.title contains '3 Pack' %}
            <div class="top-seller">Top-seller</div>
          {% endif %}
          {% if variant.title != '1 Pack'
            and variant.title != '1 Pack / Gentle'
            and variant.title != '1 Pack /
                  Extra'
          %}
            <div class="sale">{{ sale }}</div>
          {% endif %}
          <p class="months">{{ month }}</p>
        </div>
      </div>
    {% endfor %}
  </div>
  {% for variant in product.variants %}
    {% assign comparePrice = variant.compare_at_price | plus: 0 %}
    {% assign price = variant.price | divided_by: 100.0 %}
    {% assign active = '' %}

    {% if variant.title contains '12 Pack' %}
      {% assign sale = 'SAVE 30%' %}

    {% elsif variant.title contains '3 Pack' %}
      {% assign sale = 'SAVE 22%' %}

    {% elsif variant.title contains '1 Pack' %}
      {% assign sale = '' %}
    {% endif %}

    {% if variant.title == '3 Pack' or variant.title == '3 Pack / Gentle' %}
      {% assign active = 'active' %}
    {% endif %}

    {% if active == 'active' %}
      <div class="footer-prices">
        <p>Total price</p>

        <div class="d-flex items-center">
          <p class="pr">{{ variant.price | money }}</p>
          <p class="sale d-flex items-center">
            <span class="">{{ sale }}</span>
            <span class="tooltip-wrap">
              <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path
                    d="M6 0C2.68548 0 0 2.70968 0 6C0 9.31452 2.68548 12 6 12C9.29032 12 12 9.31452 12 6C12 2.70968 9.29032 0 6 0ZM6 2.66129C6.55645 2.66129 7.01613 3.12097 7.01613 3.67742C7.01613 4.25806 6.55645 4.69355 6 4.69355C5.41935 4.69355 4.98387 4.25806 4.98387 3.67742C4.98387 3.12097 5.41935 2.66129 6 2.66129ZM7.35484 8.80645C7.35484 8.97581 7.20968 9.09677 7.06452 9.09677H4.93548C4.76613 9.09677 4.64516 8.97581 4.64516 8.80645V8.22581C4.64516 8.08064 4.76613 7.93548 4.93548 7.93548H5.22581V6.3871H4.93548C4.76613 6.3871 4.64516 6.26613 4.64516 6.09677V5.51613C4.64516 5.37097 4.76613 5.22581 4.93548 5.22581H6.48387C6.62903 5.22581 6.77419 5.37097 6.77419 5.51613V7.93548H7.06452C7.20968 7.93548 7.35484 8.08064 7.35484 8.22581V8.80645Z"
                    fill="white"></path>
              </svg>
              <span class="tooltip"
                >Total discount from the à la carte price of <span class="compare">{{ comparePrice | money }}</span>:
                you save $<span class="save">
                  {{- variant.compare_at_price | plus: 0 | minus: variant.price | divided_by: 100.0 | round: 0 -}}
                </span>
                on the big pack. You can save 10% more with auto delivery.</span
              >
            </span>
          </p>
        </div>
      </div>
    {% endif %}
  {% endfor %}

  {% if section.settings.on_country != blank %}
    {% assign country = section.settings.on_country | split: ',' %}
    <span>Ship to:</span>
    <select class="country_select" name="country">
      {% for c in country %}
        {% assign cval = c | split: '/' %}
        {% assign minDay = cval[1] | split: '-' %}
        {% assign minday = minDay[0] | times: 24 | times: 60 | times: 60 %}
        {% assign maxday = minDay[1] | times: 24 | times: 60 | times: 60 %}

        <option
          value="{{ cval[0] }}"
          data-free="{{ cval[2] }}"
          {% if forloop.firsy %}
            selected
          {% endif %}
          data-value="
            {{ "now" | date: "%s" | plus: minday | date: "%d %b" }} - {{ "now" | date: "%s" | plus: maxday
            | date: "%d %b" }}
          "
        >
          {{ cval[0] }}
        </option>
      {% endfor %}</select
    ><br>
    {% assign cval = country[0] | split: '/' %}
    {% assign minDay = cval[1] | split: '-' %}
    {% assign minday = minDay[0] | times: 24 | times: 60 | times: 60 %}
    {% assign maxday = minDay[1] | times: 24 | times: 60 | times: 60 %}
    <span class="delivery_date"
      >Arrives:
      <b>
        {{- 'now' | date: '%s' | plus: minday | date: '%d %b' }} -
        {{ 'now' | date: '%s' | plus: maxday | date: '%d %b' -}}
      </b></span
    ><br>
  {% endif %}
  <span class="free-shipping-label">Free shipping above $50</span>

  <h4 class="stock__header">In Stock.</h4>
  <select class="stock__select">
    {% for c in (1..20) %}
      <option value="{{c}}">Qty: {{ c }}</option>
    {% endfor %}
  </select>
  <span class="stock__pack">1 pack = <span>84</span> strips</span>

  <label class="subscribe-custom" style="display: block;">
    <input class="subscribe-custom__checkbox" type="checkbox">
    <span class="check-customer"></span>
    <h5 class="subscribe-custom__header">Subscribe and <span>save 10%</span></h5>
    <span class="subscibe-custom__info">Auto delivery every 3 month for $50.37. No hidden fees. Сancel anytime.</span>
  </label>
  <button class="to_checkout" data-product="{{ product.id }}">Add to cart</button>
  <div class="moneyback">
    <img src="https://i.ibb.co/kXqgv3n/image-44-2.png" alt="moneyback">
    <p>
      30-day <br>
      Money-Back <br>
      Guarantee
    </p>
  </div>
  <ul class="checklist">
    <li>Promote nose breathing</li>
    <li>Reduce open-mouth snoring</li>
    <li>Boost CPAP exprerience</li>
    <li>Improve sleep quality</li>
  </ul>
</div>

<script>

  document.addEventListener('DOMContentLoaded', function () {
  let currency = '$';
  let priceSubscribe = (+$('.mobile .swatchCustom__item_new.active').data('price') - (+$('.mobile .swatchCustom__item_new.active').data('price') * 10 / 100)).toFixed(2);
  $('.mobile .subscibe-custom__info').text(`Auto delivery every 3 month for ${currency + priceSubscribe}. No hidden fees. Сancel anytime.`)
  // let sellingPlanGroups = ''
  //  fetch(window.location.pathname + '.js')
  //   .then(response => response.json())
  //   .then(data => { 
  //       console.log(data)
  //       sellingPlanGroups = data['selling_plan_groups'][0]['app_id']

  //     let variants = data.variants;
  //     for (let i = 0; i < variants.length; i++) {
  //         document.querySelectorAll('.swatchCustom__item_new').forEach(item => {
  //           if (variants[i].id == item.dataset.variant && !item.dataset.title.includes('1 Pack')) {
  //             item.dataset.planid = variants[i]['selling_plan_allocations'][0]['selling_plan_id']
  //           }
  //         })
  //     }

  //   });
  let matMedia = window.matchMedia("(max-width: 1023px)").matches ? '.mobile' : '.desktop';
  let matMediaFun = () => {
      matMedia = window.matchMedia("(max-width: 1023px)").matches ? '.mobile' : '.desktop';
  }

  window.addEventListener('resize', () => matMediaFun())



  // select delivery country

  $('.product__information .country_select').change(function () {
      $('.product__information .delivery_date b').text($(this).find(':selected').data('value'))
      console.log()
      if ($(this).find(':selected').data('free') !== 'free') {
          $('.product__information .free-shipping-label').text(`Shipping from ${$(this).find(':selected').data('free')}`)
      } else {
          $('.product__information .free-shipping-label').text(`Free Shipping`)
          if($(this).find(':selected').val() === 'United States') {
            $('.product__information .free-shipping-label').text(`Free shipping above $50`)
          }
      }
  })



  // for mobile blocks 

  $('.select_pack').click(function () {
      $('.dark_bg').addClass('active')
  })

  $('.close_btn').click(function () {
      $('.dark_bg').removeClass('active')
  })

  $(`.subscribe-custom__checkbox`).change(function (e) {
      clearInterval(intervalSubscribePDP)
      let sale = $(`${matMedia} .parent-items .swatchCustom__item_new.active`).find('.sale').text();
      let price = $(`${matMedia} .parent-items .swatchCustom__item_new.active`).data(`price`);
      let priceSubscribe = (+price - (+price * 10 / 100)).toFixed(2);

      if (e.currentTarget.checked) {
          $(`${matMedia} .stock__select`).prop('disabled', 'true');
          $(`${matMedia} .stock__select`).val('1');


          if ($(`${matMedia} .parent-items .swatchCustom__item_new.active`).data('title').includes('3 Pack')) {
              if (matMedia == '.mobile') {
                  $('.footer-prices .pr').text(currency + priceSubscribe)
                  $('.footer-prices .sale > span:first-child').text('Save 30%')
              } else {
                  $('.total_price span').text(currency + priceSubscribe)
                  $('.part2 .sale > span:first-child').text('Save 30%');
              }
          } else {
              if (matMedia == '.mobile') {
                  $('.footer-prices .pr').text(currency + priceSubscribe)
                  $('.footer-prices .sale > span:first-child').text('Save 37%')

              } else {
                  $('.total_price span').text(currency + priceSubscribe)
                  $('.part2 .sale > span:first-child').text('Save 37%');

              }
          }
      } else {
          $(`${matMedia} .stock__select`).removeAttr('disabled')

          if (matMedia == '.mobile') {
              $('.footer-prices .pr').text(currency + price)
              $('.footer-prices .sale > span:first-child').text(sale)

          } else {
              $('.total_price span').text(currency + price)
              $('.part2 .sale > span:first-child').text(sale);

          }
      }
  })

  $(`.parent-items .swatchCustom__item_new`).click(function () {
      console.log(matMedia)
      if (!$(this).hasClass('active')) {
          $(this).siblings().removeClass('active')
          $(this).addClass('active')
          $(`${matMedia} .stock__select`).prop("disabled", false)
          $(`${matMedia} .subscribe-custom__checkbox`).prop("checked", false)

          let price = $(this).data('price')
          let total = $(this).find('.item_total > span:not(.l-through)').html();

          if (matMedia == '.mobile') {
              $('.footer-prices .pr').html(total)
          } else {
              $('.total_price span').text(currency + price)
          }

          $(`${matMedia} .stock__select`).val('1')


          let priceSubscribe = (+price - (+price * 10 / 100)).toFixed(2);

          if ($(this).data('title').includes('3 Pack') || $(this).data('title').includes('12 Pack')) {

              let compare = $(this).find('.item_total > span.l-through').html();
              let save = +compare.replace(currency, '') - +total.replace(currency, '');

              $(`${matMedia} .sale .tooltip .compare`).text(compare)
              $(`${matMedia} .sale .tooltip .save`).text(save)
          }
          if ($(this).data('title').includes('3 Pack')) {
              $(`${matMedia} .subscribe-custom`).css('display', 'block')
              $(`${matMedia} .stock__pack span`).text('84')
              $(`${matMedia} .subscibe-custom__info`).text(`Auto delivery every 3 month for ${currency + priceSubscribe}. No hidden fees. Сancel anytime.`)

              if (matMedia == '.desktop') {
                  $(`.subheading`).text(`12 week pack (84 strips in a pack)`);
                  $('.part2 .sale > span:first-child').text('SAVE 22%');
                  $('.part2 .sale').css('display', 'flex')
              } else {
                  $('.footer-prices .sale > span:first-child').text('SAVE 22%');
                  $('.footer-prices .sale').css('display', 'flex')
              } 


          } else if ($(this).data('title').includes('12 Pack')) {
              $(`${matMedia} .subscribe-custom`).css('display', 'block')
              $(`${matMedia} .stock__pack span`).text('364')
              $(`${matMedia} .subscibe-custom__info`).text(`Auto delivery every 12 month for ${currency + priceSubscribe}. No hidden fees. Сancel anytime.`)

              if (matMedia == '.desktop') {
                  $('.subheading').text(`52 week pack (364 strips in a pack)`);
                  $('.part2 .sale > span:first-child').text('SAVE 30%');
                  $('.part2 .sale').css('display', 'flex')
              } else {
                  $('.footer-prices .sale > span:first-child').text('SAVE 30%');
                  $('.footer-prices .sale').css('display', 'flex')
              }
          } else {
              $(`${matMedia} .subscribe-custom`).css('display', 'none')
              $(`${matMedia} .stock__pack span`).text('28')
              if (matMedia == '.desktop') {
                  $('.subheading').text(`4 week pack (28 strips in a pack)`);
                  $('.part2 .sale').css('display', 'none')
              } else {
                  $('.footer-prices .sale').css('display', 'none')
              }
          }
      } else {
          if (matMedia == '.mobile') {
              $('.popup_btn').eq(1).click()
          } else if (matMedia == '.desktop') {
              $('.popup_btn').eq(0).click()
          }

      }
  })

//to checkout

  $('.to_checkout').click(async function () {
      try {
          let matMedia = window.matchMedia("(max-width: 1023px)").matches ? '.mobile' : '.desktop';

          const itemId = $(`${matMedia} .parent-items .swatchCustom__item_new.active`).data('variant');
          const planId = $(`${matMedia} .parent-items .swatchCustom__item_new.active`).data('planid')
          const itemQuantity = $(`${matMedia} .stock__select`).val();
          const productId = $(this).data('product');

          let sale = 0;
          let add = true
          let compare = $(`${matMedia} .parent-items .swatchCustom__item_new.active`).data('title') == '1 Pack' ? 0 : $(`${matMedia} .parent-items .swatchCustom__item_new.active`).data('compare').replace(currency, '');
          if ($(`${matMedia} .parent-items .swatchCustom__item_new.active`).data('title') == '3 Pack') {
              sale = document.querySelector(`${matMedia} .subscribe-custom__checkbox`).checked ? 30 : 22;
          } else if ($(`${matMedia} .parent-items .swatchCustom__item_new.active`).data('title') == '12 Pack') {
              sale = document.querySelector(`${matMedia} .subscribe-custom__checkbox`).checked ? 37 : 30;
          }

          $('.loader_custom').addClass('active')
          let formData = {
              'items': [{
                  'id': itemId,
                  'quantity': itemQuantity
              }]
          };
          if (document.querySelector(`${matMedia} .subscribe-custom__checkbox`).checked) {
              formData = {
                  items: [{
                      id: itemId,
                      quantity: itemQuantity,
                      selling_plan: planId
                  }]
              }
              const cart = await fetch('/cart.js').then(res => res.json())

              if (cart.items.length == 0) {
                  const addCart = await addToCart(formData)
                  window.location.href = '/cart'
              } else {
                  cart.items.forEach(item => {
                      if (item.selling_plan_allocation && item.selling_plan_allocation.selling_plan.id == planId) {
                          add = false
                      }
                  })

                  if (add) {
                      const addCart = await addToCart(formData)
                      window.location.href = '/cart'
                  } else {
                      window.location.href = '/cart'
                  }
              }
          } else {
              const addCart = await addToCart(formData)
              window.location.href = '/cart'
          }
          $('.loader_custom').removeClass('active')


      } catch (e) {
          console.log(e)
      }

  })

  // calculating the total price
  $(`.stock__select`).change(function () {
      let price = +$(`${matMedia} .swatchCustom__item_new.active`).data('price')
      let total = (price * document.querySelector(`${matMedia} .stock__select`).value).toFixed(2)
      if (matMedia == '.desktop') {
          $(`.total_price span`).text(currency + total)
      } else {
          $('.footer-prices .pr').text(currency + total)
      }
  })

  let intervalSubscribePDP = setInterval(() => {
      if ($(".mobile .subscribe-custom .subscribe-custom__checkbox").is(':checked')) {
          clearInterval(intervalSubscribePDP)
          $(".mobile .subscribe-custom .subscribe-custom__checkbox").prop('checked', false);
      }
  }, 200)


  document.querySelectorAll('.part1 i.stamped-fa-star').forEach(item => {
      item.style.cssText = 'color: rgb(242, 180, 19) !important;'
  })

  {% if customer != blank %}

  var cid = "{{ customer.id }}";
  localStorage.setItem('customer', cid);

  fetch('https://crs-dev.fun/api/somnifix/recharge/get_subscriptions', {
      method: 'POST',
      headers: { 'Content-Type': 'application/json' },
      body: JSON.stringify({
          'params': {
              "shopify_customer_id": cid,
              "status": "active"
          }
      })
  }).then(res => res.json()).then(data => {
      console.log(data)
      if (data.errors || data.subscriptions.length === 0) {
          dataLayer.push({ 'event': 'popup_start' });
      }
  })

  {% else %}
  localStorage.removeItem('customer');
  dataLayer.push({ 'event': 'popup_start' });
  {% endif %}


  document.querySelectorAll('.to_checkout').forEach((item, i) => {
      item.addEventListener('click', function () {
          if (document.querySelectorAll('.subscribe-custom__checkbox')[i].checked) {
              dataLayer.push({ 'event': 'buying_subscription' });
          }
      })
  })


  async function addToCart(item) {
      return await (fetch('/cart/add.js', {
          method: 'POST',
          headers: {
              'Content-Type': 'application/json'
          },
          body: JSON.stringify(item)
      }).then(res => res.json()))
  }

});
</script>
