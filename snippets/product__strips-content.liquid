    .tooltip-wrap .tooltip {
      position: absolute;
      top: calc(100% + 5px);
      right: -8px;
      background: #FFFFFF;
      box-shadow: 0px 2px 14px rgba(0, 0, 0, 0.15);
      border-radius: 10px;
      font-family: 'Roboto';
